package Exercicios6;

public class Cavalo extends Animal {

	public void nome()
	{
		System.out.println("Cavalo");
	}
	
	public void idade(){
		
		System.out.println("20 anos");
	}
	
	public void emitirSom(){
		
		System.out.println("Relinchar");
	}
	
	public void correr(boolean x){
		
		if(x==true)
			System.out.println("Correndo!");
		else
			System.out.println("Saltando obstáculos");
	}
	
}
